{
  "name": "dllspot",
  "language": ["hi", "en"],
  "domains": ["ddlspot.com"],
  "base_url": "http://www.ddlspot.com",
  "search_url_format_episode": {
    "string_format": "/search/?q=%s+S%02dE%02d&m=1&x=0&y=0",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search/?q=%s+%04d&m=1&x=0&y=0",
    "replacement": [
      "title",
      "year_int"
    ]
  },
  "more_pages_indicator": ">Next Page",
  "next_page_path" : "div.box-content a[title]:contains(Next Page)",
  "next_page_path_attr": "abs:href",
  "links_on_first_page": false,
  "drill_down_path": "td.c a[title]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": false,
  "is_direct": true,
  "links_container_path": "div.dl-links a",
  "link_path": "a[href]",
  "link_path_attr": "",
  "title_path": "/div#header + div.tcl + span.headings",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "",
  "magnet_path_attr": "",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "/div.left strong:eq(6)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
