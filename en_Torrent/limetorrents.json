{
    "name": "limetorrents",
    "language": ["hi", "en"],
    "domains": ["limetorrents.info", "limetorrents.unblockit.onl", "limetorrents.pro", "limetorrent.cc", "limetorrentx.cc"],
    "base_url": "https://www.limetorrents.lol",
    "search_url_format_episode": {
      "string_format": "/search/tv/%s-s%02de%02d",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/search/movies/%s-%04d",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "more_pages_indicator": "Next page",
    "next_page_path" : "div.search_stat a#next",
    "next_page_path_attr": "abs:href",
    "links_on_first_page": false,
    "drill_down_path": "table.table2 tr a:not([rel])",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "div.dltorrent a:contains(magnet download)",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "/div#maincontentrouter h1",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a:contains(magnet download)",
    "magnet_path_attr": "href",
    "seeders_path": "/div#maincontentrouter span.greenish:contains(seeders)",
    "seeders_path_attr": "",
    "filesize_path": "/div.torrentinfo table tr:contains(torrent size) td:gt(0)",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
