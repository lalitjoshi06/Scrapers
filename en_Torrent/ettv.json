{
  "name": "ettv",
  "language": ["hi", "en"],
  "domains": ["ettvcentral.com", "ettv.unblockedfor.me"],
  "base_url": "https://ettv.unblockedfor.me",
  "search_url_format_episode": {
    "string_format": "/torrents-search.php?search=%s+S%02dE%02d",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/torrents-search.php?search=%s+%04d",
    "replacement": [
      "title",
      "year_int"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path" : "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "table tr td a[title]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div#downloadbox",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "/fieldset.download table tr:eq(0) td:eq(1)",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "table tr td:eq(0) a[href]",
  "magnet_path_attr": "href",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "/fieldset.download td:contains(Total Size) + td",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
