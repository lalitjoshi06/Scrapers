{
	"name": "CloudTorrent",
	"language": [
		"en",
		"hi"
	],
	"domains": [
		"cloudtorrents.com"
	],
	"base_url": "https://cloudtorrents.com",
	"search_url_format_episode": {
		"string_format": "/search?ordering=-uploaded&query=%s+S%02dE%02d",
		"replacement": [
			"title",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/search?ordering=-uploaded&query=%s+%04d",
		"replacement": [
			"title",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/search?ordering=-uploaded&query=%s",
		"replacement": [
			"title_lower"
		]
	},
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": true,
	"drill_down_path": "",
	"drill_down_path_attr": "",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "tbody tr",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "td.torrent-title a",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "a[href^=magnet]",
	"magnet_path_attr": "href",
	"seeders_path": "td[data-title='Se']",
	"seeders_path_attr": "",
	"filesize_path": "td[data-title='Size']",
	"filesize_path_attr": "",
	"quality_path": "td.torrent-title a",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}