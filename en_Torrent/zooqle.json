{
    "name": "zooqle",
    "language": ["hi", "en"],
    "domains": ["zooqle.com", "zooqle.unblockedfor.me"],
    "base_url": "https://zooqle.com",
    "search_url_format_episode": {
      "string_format": "/search?pg=1&q=%s+s%02de%02d",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/search?pg=1&q=%s+%04d",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "more_pages_indicator": "<ul class=\"pagination smaller",
    "next_page_path" : "ul.pagination li.active + li a",
    "next_page_path_attr": "abs:href",
    "links_on_first_page": false,
    "drill_down_path": "a[class=small]:not(a[role=menuitem])",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "li:has(a[href^=magnet])",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "/h4#torname",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href^=magnet]",
    "magnet_path_attr": "href",
    "seeders_path": "/h6 div.progress > div:first-of-type",
    "seeders_path_attr": "",
    "filesize_path": "/h6:has(i[title^=file]) i[title^=file]",
    "filesize_path_attr": "|next_sibling|",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
  
