{
    "name": "isohunt",
    "language": [
      "hi",
      "en"
    ],
    "domains": [
      "isohunt.nz"
    ],
    "base_url": "https://isohunt.app",
    "search_url_format_episode": {
      "string_format": "/torrent/?ihq=%s+s%02de%02d&fiht=2&age=0&Torrent_sort=seeders&Torrent_page=0",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/torrent/?ihq=%s+%04d&fiht=2&age=0&Torrent_sort=seeders&Torrent_page=0",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },  
    "check_title_before_drill_down": true,
    "drill_down_title_check_attr": "span",
    "max_more_pages": 10,
    "more_pages_indicator": "ul.pagination li.next",
    "next_page_path": "ul.pagination li.next a",
    "next_page_path_attr": "href",    
    "links_on_first_page": false,
    "drill_down_path": "table tbody tr[data-key=0] a[href*=/torrent_details]",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "div.col-sm-12 div.p",
    "link_path": "",
    "link_path_attr": "",
    "title_path": " a[href*=/torrent_details]",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href*=magnet]:eq(0)",
    "magnet_path_attr": "href",
    "seeders_path": "span.seeds",
    "seeders_path_attr": "",
    "filesize_path": "p.text-lg:contains(Size)",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }