{
  "name": "Commet",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "elfhosted.com"
  ],
  "base_url": "https://comet.stremio.ru",
  "search_url_format_episode": {
    "string_format": "/stream/series/%s:%02d:%02d.json",
    "replacement": [
      "imdb_id",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/stream/movie/%s.json",
    "replacement": [
      "imdb_id"
    ]
  },
  "api": {
    "path_to_array": "streams",
    "path_title_in_array": "description",
    "api_title_split": "/|\u2508\u27A4",
    "path_seeders": "fileIdx",
    "path_size": "behaviorHints{}videoSize",
    "path_quality": "behaviorHints{}filename",
    "path_hash": "infoHash"
  },
  "is_torrent": true,
  "is_direct": false
}