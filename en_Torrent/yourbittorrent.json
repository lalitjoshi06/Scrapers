{
    "name": "yourbittorrent",
    "language": ["en"],
    "domains": ["yourbittorrent2.com", "yourbittorrent.com"],
    "base_url": "https://sitenable.ch/o.php?b=21&f=norefer&pv=0&mobile=&u=https://yourbittorrent2.com",
    "search_url_format_episode": {
      "string_format": "?q=%s+s%02de%02d&page=1&v=&c=3&sort=size&o=desc",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "?q=%s+%04d&page=1&v=&c=1&sort=size&o=desc",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "more_pages_indicator": "Next",
    "next_page_path" : "",
    "next_page_path_attr": "",
    "links_on_first_page": false,
    "drill_down_path": "tr.table-default a",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "div:has(kbd)",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "/h3.card-title",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "kbd",
    "magnet_path_attr": "|hash|",
    "seeders_path": "/div.row div.col-3:contains(seeders) + div b",
    "seeders_path_attr": "",
    "filesize_path": "/div.row div.col-3:contains(file size) + div",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
  
