{
  "name": "magnetdl",
  "language": ["hi", "en"],
  "domains": ["magnetdl.com", "magnetdl.unblockit.buzz", "magnetdl.unblockedfor.me"],
  "base_url": "https://magnetdl.com",
  "search_url_format_episode": {
    "string_format": "/%.1s/%s-s%02de%02d/se/desc/",
    "replacement": [
      "title_lower",
      "title_lower_dash",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/%.1s/%s-%d/se/desc/",
    "replacement": [
      "title_lower",
      "title_lower_dash",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/%.1s/%s",
    "replacement": [
      "title_lower",
      "title_lower_dash"
    ]
  },
  "more_pages_indicator": "Next Page",
  "next_page_path" : "",
  "next_page_path_attr": "",
  "links_on_first_page": true,
  "drill_down_path": "",
  "drill_down_path_attr": "",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "tbody tr",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "tr td.n a[title]",
  "title_path_attr": "title",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "tr td.m a[href]",
  "magnet_path_attr": "href",
  "seeders_path": "tr td.s",
  "seeders_path_attr": "",
  "filesize_path": "tr td:eq(5)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
